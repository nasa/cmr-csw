                        &lt;ogc:Literal&gt;1990-09-03T00:00:01Z&lt;/ogc:Literal&gt;
                    &lt;/ogc:PropertyIsGreaterThanOrEqualTo&gt;
                    &lt;ogc:PropertyIsLessThanOrEqualTo&gt;
                        &lt;ogc:PropertyName&gt;TempExtent_end&lt;/ogc:PropertyName&gt;
